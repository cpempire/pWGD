mpirun -n 1 python mcmc_dili.py

for i in $(seq 1 10)
do
	mpirun -n 16 python WGF.py

	cp data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_False_WGF.p data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_False_WGF_$i.p
done

for i in $(seq 1 10)
do
	mpirun -n 16 python pWGF.py

	cp data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_True_WGF.p data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_True_WGF_$i.p
done

for i in $(seq 1 10)
do
  mpirun -n 16 python SVGD.py

	cp data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_False_SVGD.p data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_False_SVGD_$i.p
done

for i in $(seq 1 10)
do
  mpirun -n 16 python pSVGD.py

	cp data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_True_SVGD.p data/data_nDimensions_81_nCores_16_nSamples_256_isProjection_True_SVGD_$i.p
